{
    "name": "tekstove/tekstove-api",
    "license": "proprietary",
    "type": "project",
    "description": "tekstove.info api",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/expression-language": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/web-link": "^4.1",
        "symfony/webpack-encore-pack": "*",
        "symfony/yaml": "^4.1",

        "propel/propel": "dev-master",
        "propel/propel-bundle": "4.0.*-dev",
        "friendsofsymfony/rest-bundle": "^2.3.1",
        "jms/serializer-bundle": "~2.4.0",
        "predis/predis": "^1.1.1",
        "potaka/knp-paginator-propel2bundle": "0.* || 1.*",
        "potaka/bbcode-bundle": "^0.2.0",
        "potaka/helper-casing": "^0.1.0",
        "google/recaptcha": "^1.1",
        "tekstove/content-checker": "~0.3 || 1.*",
        "tekstove/url-video-parser": "^1.0.1",
        "potaka/ip-anonymizer": "^2.0"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",

        "phpunit/phpunit": "~5.4",
        "mockery/mockery": "^0.9.9"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": false
    },
    "autoload": {
        "psr-4": {
            "Tekstove\\ApiBundle\\": "src/Tekstove/ApiBundle/",
            "App\\": "src/"
        },
        "classmap": []
    },
    "autoload-dev": {
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
